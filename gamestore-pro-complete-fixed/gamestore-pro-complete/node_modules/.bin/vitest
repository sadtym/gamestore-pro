#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/tmp/gamestore-pro-complete/.pnpm/vitest@0.34.6/node_modules/vitest/node_modules:/tmp/gamestore-pro-complete/.pnpm/vitest@0.34.6/node_modules:/tmp/gamestore-pro-complete/.pnpm/node_modules"
else
  export NODE_PATH="/tmp/gamestore-pro-complete/.pnpm/vitest@0.34.6/node_modules/vitest/node_modules:/tmp/gamestore-pro-complete/.pnpm/vitest@0.34.6/node_modules:/tmp/gamestore-pro-complete/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
